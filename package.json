{
    "name": "spineedit",
    "version": "1.0.0",
    "description": "A simple SVG editor for single-stroke (SPINE) diagrams with editing and optimization capabilities.",
    "main": "js/app.js",
    "scripts": {
        "start": "npm-run-all --parallel serve watch:css",
        "serve": "npx webpack && webpack serve --mode development --open",
        "build": "npx webpack",
        "watch:css": "sass --watch scss/style.scss:css/style.css"
    },
    "keywords": [
        "SVG",
        "editor",
        "SPINE",
        "single-stroke",
        "optimization"
    ],
    "author": "Herbert Spencer Gonz√°lez",
    "license": "GPL-3.0",
    "dependencies": {
        "@svgdotjs/svg.draggable.js": "^3.0.4",
        "@svgdotjs/svg.js": "^3.2.4"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "babel-loader": "^9.2.1",
        "clean-css-cli": "^5.0.0",
        "css-loader": "^7.1.2",
        "html-webpack-plugin": "^5.6.3",
        "live-server": "^1.2.0",
        "mini-css-extract-plugin": "^2.9.2",
        "npm-run-all": "^4.1.5",
        "sass": "^1.81.0",
        "sass-loader": "^16.0.3",
        "svgo": "^3.3.2",
        "terser": "^5.0.0",
        "webpack": "^5.96.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0"
    }
}
